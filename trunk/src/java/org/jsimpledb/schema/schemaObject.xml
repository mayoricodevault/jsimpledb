<binding>
    <include path="abstractSchemaItem.xml"/>
    <include path="fields.xml"/>
    <mapping abstract="true" type-name="schemaObject" class="org.jsimpledb.schema.SchemaObject">
        <structure map-as="abstractSchemaItem"/>
        <collection item-type="org.jsimpledb.schema.SchemaField"
          add-method="addSchemaField" iter-method="iterateSchemaFields"/>
    </mapping>
</binding>

