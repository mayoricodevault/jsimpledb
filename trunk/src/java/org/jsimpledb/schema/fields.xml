<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- $Id$ -->
<binding>

    <include path="referenceSchemaField.xml"/> 
    <include path="simpleSchemaField.xml"/> 
    <include path="setSchemaField.xml"/> 
    <include path="listSchemaField.xml"/> 
    <include path="mapSchemaField.xml"/> 

    <mapping abstract="true" class="org.jsimpledb.schema.SchemaField"/>

    <mapping name="SimpleField" class="org.jsimpledb.schema.SimpleSchemaField"
      extends="org.jsimpledb.schema.SchemaField">
        <structure map-as="simpleSchemaField"/>
    </mapping>

    <mapping name="ReferenceField" class="org.jsimpledb.schema.ReferenceSchemaField"
      extends="org.jsimpledb.schema.SimpleSchemaField">
        <structure map-as="referenceSchemaField"/>
    </mapping>

    <mapping name="SetField" class="org.jsimpledb.schema.SetSchemaField"
      extends="org.jsimpledb.schema.SchemaField">
        <structure map-as="setSchemaField"/>
    </mapping>

    <mapping name="ListField" class="org.jsimpledb.schema.ListSchemaField"
      extends="org.jsimpledb.schema.SchemaField">
        <structure map-as="listSchemaField"/>
    </mapping>

    <mapping name="MapField" class="org.jsimpledb.schema.MapSchemaField"
      extends="org.jsimpledb.schema.SchemaField">
        <structure map-as="mapSchemaField"/>
    </mapping>
</binding>

